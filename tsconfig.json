{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "rootDir": "./",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck":true,
    "types": ["vite/client"],
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": "./"
  }
}
